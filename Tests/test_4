main
var input, myVar;
function getSumToN(n)
{
	if n == 1 then
		return 1;
	fi;
    return call getSumToN(n-1) + n;
};

{
    let input <- call inputnum();
    let myVar <- call getSumToN(input);
    call outputnewline();
	call outputnum(input);
    call outputnewline();
	call outputnum(myVar);
	call outputnewline();
}.
