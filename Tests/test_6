main
var input, myvar;
function fibonacci(n)
{
	if n == 1 then
		return 1;
	fi;
	
	if n == 0 then
		return 0;
	fi;
	
    return call fibonacci(n-1) + call fibonacci(n-2);
};

{
    let input <- call inputnum();
    let myvar <- call fibonacci(input);
    call outputnewline();
    call outputnum(input);
    call outputnewline();
    call outputnum(myvar);
    call outputnewline();
}.
