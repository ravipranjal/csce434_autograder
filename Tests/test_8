main
var input, myvar;
function getnumberfrominput(n)
var num, next;
{	
    
	let num <- call inputnum();
    
    call outputnum(num);    
    call outputnewline();
	if n == 1 then
		return num;
	fi;
    
    let next <- call getnumberfrominput(n-1);
    let next <- next*10;
    return num + next;
};

{
    let input <- call inputnum();
    call outputnewline();
    let myvar <- call getnumberfrominput(input);
    call outputnewline();
    call outputnum(myvar);
    call outputnewline();
}.
