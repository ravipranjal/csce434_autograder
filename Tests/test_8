main
var input, myVar;
function getNumberFromInput(n)
var num, next;
{	
    
	let num <- call inputnum();
    
    call outputnum(num);    
    call outputnewline();
	if n == 1 then
		return num;
	fi;
    
    let next <- call getNumberFromInput(n-1);
    let next <- next*10;
	return num + next;
};

{
    let input <- call inputnum();
    
    call outputnewline();
    let myVar <- call getNumberFromInput(input);
    call outputnewline();
	call outputnum(myVar);
	call outputnewline();
}.
