main
var input, myvar;
function maxnumberfrominput(n)
var max, i, curr;
{	
	let max <- 0;
	let i <- 0;
	
	while i < n do
		let curr <- call inputnum();
		if curr > max then
			let max <- curr;
		fi;
		let i <- i + 1;
	od;
    return max;
};

{
    let input <- call inputnum();
    let myvar <- call maxnumberfrominput(input);
    call outputnewline();
    call outputnum(myvar);
    call outputnewline();
}.
