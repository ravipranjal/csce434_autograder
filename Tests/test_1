main
var x;
{
	let x <- call inputnum();
	call outputnum(x);
	call outputnewline();
    if x < 2 then
        call outputnum(x);
        call outputnewline();
    else 
        call outputnum(100);
        call outputnewline();
    fi;
    call outputnum(-1);
    call outputnewline();
}.
