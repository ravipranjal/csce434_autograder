main
var x;
{
	let x <- call inputnum();
	call outputnum(x);
	call outputnewline();
    if x < 2 then
        call outputnum(x*2);
        call outputnewline();
    else 
        call outputnum(100*x);
        call outputnewline();
    fi;
    call outputnum(10+1);
    call outputnewline();
}.
