main
var input;

function factIter(n)
var i, f;
{
    let i <- 1;
    let f <- 1;
    while i <= n do
        let f <- f * i;
        let i <- i + 1;
    od;
    return f;
};

function factRec(n)
{
    if n <= 1 then
        return 1;
    fi;
    return call factRec(n - 1) * n;
};

{
    let input <- call inputnum();
    call outputnewline();
    call outputnum(call factIter(input));
    call outputnewline();
    call outputnum(call factRec(input));
    call outputnewline();
}.
