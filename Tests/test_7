main
var input;

function factiter(n)
var i, f;
{
    let i <- 1;
    let f <- 1;
    while i <= n do
        let f <- f * i;
        let i <- i + 1;
    od;
    return f;
};

function factrec(n)
{
    if n <= 1 then
        return 1;
    fi;
    return call factrec(n - 1) * n;
};

{
    let input <- call inputnum();
    call outputnewline();
    call outputnum(call factiter(input));
    call outputnewline();
    call outputnum(call factrec(input));
    call outputnewline();
}.
