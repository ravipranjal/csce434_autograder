main
var input, myvar;
procedure printnum(n)
var i;
{	
	if n > 0 then
    	let i <- 0;
	    while i < n do
		    call outputnum(1);
    		let i <- i + 1;
	    od;
    	call outputnewline();
	    call printnum(n-1);	
	fi;
};

{
    let input <- call inputnum();
    call outputnewline();
    let myvar <- call printnum(input);
    call outputnewline();
}.
