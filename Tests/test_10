main
var input, myVar;
function printNum(n);
var i;
{	
	if n == 0 then
		return 0;
	fi;
	
	let i <- 0;
	while i < n do
		call outputnum(1);
		let i <- i + 1;
	od;
	call outputnewline();
	call printNum(n-1);	
	return 0;
};

{
    let input <- call inputnum();
	call outputnewline();
    let myVar <- call printNum(input);
    call outputnewline();
}.