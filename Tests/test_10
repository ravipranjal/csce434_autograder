main
var input, myVar;
procedure printNum(n)
var i;
{	
	if n > 0 then
    	let i <- 0;
	    while i < n do
		    call outputnum(1);
    		let i <- i + 1;
	    od;
    	call outputnewline();
	    call printNum(n-1);	
	fi;
};

{
    let input <- call inputnum();
	call outputnewline();
    let myVar <- call printNum(input);
    call outputnewline();
}.
