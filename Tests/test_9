main
var input, myVar;
function tailRecurSum(n,i);
{	
	if n == 0 then
		return i;
	fi;
	return call tailRecurSum(n-1, i + n);
};

{
    let input <- call inputnum();
    let myVar <- call tailRecurSum(input, 0);
    call outputnewline();
	call outputnum(myVar);
	call outputnewline();
}.
