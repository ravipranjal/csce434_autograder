main
var x,y,z;
{
	let x <- call inputnum();
	let y <- call inputnum();
	let z <- call inputnum();
	call outputnum(x);
	call outputnewline();
	call outputnum(y);
	call outputnewline();
	call outputnum(z);
	call outputnewline();
    if x > y then
    	call outputnum(x+y/z);
    	call outputnewline();
    fi;
}.
