main
var input, myvar;
function getsumton(n)
var i,j;
{
    let i <- 0;
	let j <- 1;
    while j <= n do
		let i <- i + j;
		let j <- j + 1;
	od;
    return i;
};

{
    let input <- call inputnum();
    let myvar <- call getsumton(input);
    call outputnewline();
    call outputnum(input);
    call outputnewline();
    call outputnum(myvar);
    call outputnewline();
}.
