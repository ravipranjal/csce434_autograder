main
var csum;

function cumulativesum()
var input, sum;
{	
	let input <- call inputnum();
	call outputnum(input);
	call outputnewline();
	if input == 0 then
		return 0;
	else
        let sum <- call cumulativesum();
		return input + sum;
	fi;
};

{
	call outputnewline();
	let csum <- call cumulativesum();
	call outputnum(csum);
	call outputnewline();
}.
