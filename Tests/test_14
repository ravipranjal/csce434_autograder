main
var x,y,z;
{	
	let x <- 2;
	let y <- call inputnum();
	let z <- call inputnum();
	if x != (y-2) then
		if y+x >= z then
			call outputnum(y);
			call outputnewline();

		else
			call outputnum(z);
			call outputnewline();
		fi;
	else
		call outputnum(x);
		call outputnewline();
	fi;
}.